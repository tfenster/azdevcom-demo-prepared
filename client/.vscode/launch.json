{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Launch Edge",
            "url": "https://localhost:44445/",
            "webRoot": "${workspaceFolder}"
        }
    ]
}